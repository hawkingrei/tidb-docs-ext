{
  "name": "tidb-docs-ext",
  "version": "1.0.0",
  "mcpServers": {
    "tidb-docs-mcp": {
      "command": "uvx",
      "args": [
        "--from",
        "mcpdoc",
        "mcpdoc",
        "--urls",
        "https://raw.githubusercontent.com/hawkingrei/tidb-dev-prompt/refs/heads/main/llms.txt",
        "--allowed-domains",
        "*",
        "--transport",
        "stdio"
      ]
    }
  },
  "contextFileName": "GEMINI.md"
}